{
  "name": "tw-project",
  "version": "1.0.0",
  "description": "A starting point for an Online Store 2.0 theme using Tailwind CSS",
  "main": "index.js",
  "keywords": [
    "Shopify Theme",
    "Online Store 2.0",
    "shopify-cli",
    "Tailwind CSS",
    "Webpack"
  ],
  "author": "@devsmitin",
  "license": "ISC",
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "download": "cd shopify && shopify theme pull",
