    "publish": "webpack --mode production && cd shopify && shopify theme push"
  },
  "dependencies": {
    "tailwindcss": "^3.0.22"
  },
  "devDependencies": {
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "babel-loader": "^8.2.3",
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "css-loader": "^6.6.0",
    "style-loader": "^3.3.1",
    "postcss-loader": "^6.2.1",
    "autoprefixer": "^10.4.2",
    "sass-loader": "^12.4.0",
    "node-sass": "^7.0.1",
    "mini-css-extract-plugin": "^2.5.3"
  }
}